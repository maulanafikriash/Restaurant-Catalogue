{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-globals": 0,
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "linebreak-style": 0
    }
}
